{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": false,
    "importOrder": [
        "^@core/(.*)$",
        "^@server/(.*)$",
        "^@ui/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^api(.*)$",
        "^assets(.*)$",
        "^components(.*)$",
        "^helpers(.*)$",
        "^hooks(.*)$",
        "^services(.*)$",
        "^shared(.*)$",
        "^store(.*)$",
        "^types(.*)$",
        "^[./]"
    ],
    "jsxBracketSameLine": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 80
}
